<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<Group fx:id="group" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ProductDispenserNode">
   <children>
      <Pane fx:id="pane" prefHeight="125.0" prefWidth="100.0">
         <children>
            <ImageView fx:id="image" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
            <TextField fx:id="id" layoutY="100.0" prefHeight="25.0" prefWidth="50.0" />
            <TextField fx:id="price" layoutX="50.0" layoutY="100.0" prefHeight="25.0" prefWidth="50.0" />
         </children>
      </Pane>
   </children>
</Group>
